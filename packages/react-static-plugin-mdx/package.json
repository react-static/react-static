{
  "name": "react-static-plugin-mdx",
  "version": "7.2.0",
  "description": "A plugin for React Static to transform MDX files (Markdown + React components).",
  "main": "node.api.js",
  "repository": "https://github.com/react-static/react-static.git",
  "author": "Matteo Frana <f2@f2.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir .",
    "watch": "yarn build --watch",
    "test": "yarn format",
    "format": "prettier src/**/*.js --write",
    "preversion": "yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.3.0"
  }
}
