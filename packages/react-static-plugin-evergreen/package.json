{
  "name": "react-static-plugin-evergreen",
  "version": "7.6.0",
  "description": "A React-Static plugin that adds SSR support for evergreen",
  "main": "node.api.js",
  "repository": "https://github.com/react-static/react-static.git",
  "author": "Ugo Onali <hello@onali-ugo.fr>",
